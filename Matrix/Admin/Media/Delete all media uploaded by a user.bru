meta {
  name: Delete all media uploaded by a user
  type: http
  seq: 1
}

delete {
  url: {{protocol}}://{{delegatedURL}}/_synapse/admin/v1/users/:userId/media?limit=2500
  body: none
  auth: bearer
}

params:path {
  userId: {{userId}}
}

query {
  limit: 2500
}

auth:bearer {
  token: {{adminToken}}
}
