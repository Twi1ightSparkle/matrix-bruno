meta {
  name: Add alias to room Admin
  type: http
  seq: 1
}

put {
  url: {{protocol}}://{{delegatedURL}}/_matrix/client/r0/directory/room/:roomAlias
  body: json
  auth: bearer
}

params:path {
  roomAlias: %23alias:{{url}}
}

auth:bearer {
  token: {{adminToken}}
}

body:json {
  {
    "room_id": "{{roomId}}"
  }
}
