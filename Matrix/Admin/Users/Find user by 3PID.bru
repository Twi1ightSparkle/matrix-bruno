meta {
  name: Find user by 3PID
  type: http
  seq: 6
}

get {
  url: {{protocol}}://{{delegatedURL}}/_synapse/admin/v1/threepid/:3pidType/users/:3pid
  body: none
  auth: bearer
}

params:path {
  3pidType: {{3pidType}}
  3pid: {{3pid}}
}

auth:bearer {
  token: {{adminToken}}
}

vars:post-response {
  userId: res.body.user_id
}
