meta {
  name: Reset password
  type: http
  seq: 11
}

post {
  url: {{protocol}}://{{delegatedURL}}/_synapse/admin/v1/reset_password/:userId
  body: json
  auth: bearer
}

params:path {
  userId: {{userId}}
}

auth:bearer {
  token: {{adminToken}}
}

body:json {
  {
    "new_password": "password",
    "logout_devices": false
  }
}
