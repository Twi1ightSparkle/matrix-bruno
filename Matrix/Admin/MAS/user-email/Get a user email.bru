meta {
  name: Get a user email
  type: http
  seq: 3
}

get {
  url: {{protocol}}://{{masAdminEndpoint}}/api/admin/v1/user-emails/{{masEmailId}}
  body: none
  auth: inherit
}

script:post-response {
  const body = res.body;
  if (body.data?.attributes?.user_id) {
    bru.setEnvVar("masUserId", body.data.attributes.user_id);
  }
}
