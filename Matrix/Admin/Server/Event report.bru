meta {
  name: Event report
  type: http
  seq: 2
}

get {
  url: {{protocol}}://{{delegatedURL}}/_synapse/admin/v1/event_reports/:reportId
  body: none
  auth: bearer
}

params:path {
  reportId: 13245
}

auth:bearer {
  token: {{adminToken}}
}

vars:post-response {
  roomId: res.body.room_id,
  eventId: res.body.event_id
}
