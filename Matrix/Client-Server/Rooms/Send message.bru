meta {
  name: Send message
  type: http
  seq: 20
}

put {
  url: {{protocol}}://{{delegatedURL}}/_matrix/client/r0/rooms/:roomId/send/m.room.message/:transactionId
  body: json
  auth: bearer
}

params:path {
  roomId: {{roomId}}
  transactionId: {{$timestamp}}
}

auth:bearer {
  token: {{userToken}}
}

body:json {
  {
  	"msgtype":"m.text",
  	"body":"some_text"
  }
}
